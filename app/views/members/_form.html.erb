<%= form_for(member) do |f| %>
<%= render 'shared/error_messages', object: f.object %>

  <div class="form-group row">
    <%= f.label :numero, class: "col-sm-2 form-control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :numero, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :nombre, class: "col-sm-2 form-control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :nombre, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :email, class: "col-sm-2 form-control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :email, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :fechaNacimiento, "Fecha de Nacimiento", class: "col-sm-2 form-control-label" %>
    <div class="col-sm-10">
      <%= f.date_select :fechaNacimiento, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :presidente, class: "col-sm-2 form-control-label" %>
    <div class="col-sm-10">
      <%= f.check_box :presidente, class: "form-control" %>
    </div>
  </div>

  <div class="actions">
    <%= f.submit "Guardar", class: "btn btn-default" %>
  </div>
<% end %>
